{
    "name": "overturelabs/menu",
    "description": "Menu builder for Laravel 4",
    "keywords": ["laravel", "menu"],
    "license": "MIT",
    "authors": [
        {
            "name": "Overture Labs",
            "email": "contactus@overturelabs.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/html": "4.1.*",
        "illuminate/routing": "4.1.*",
        "illuminate/support": "4.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "dev-master@dev"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-0": {
            "OvertureLabs\\Menu\\": "src/"
        }
    },
    "minimum-stability": "dev"
}
